<T3DataStructure>
 <sheets>
  <sDEF>
   <ROOT>
     <TCEforms>
      <sheetTitle>Funktion</sheetTitle>
     </TCEforms>
     <type>array</type>
     <el>
      <switchableControllerActions>
       <TCEforms>
         <label>Select function</label>
         <config>
          <type>select</type>
          <items>
            
            <numIndex index="0">
             <numIndex index="0">Wissensbar</numIndex>
             <numIndex index="1">Category->list;Category->gbList</numIndex>
            </numIndex>
            
           </items>
         </config>
       </TCEforms>
      </switchableControllerActions>

		<settings.categorySelection>
			<TCEforms>
				<label>Start Tree</label>
				<config>
					<type>select</type>
					<renderMode>tree</renderMode>
					<treeConfig>
						<parentField>parent</parentField>
						<appearance>
							<expandAll>TRUE</expandAll>
							<showHeader>TRUE</showHeader>
						</appearance>
					</treeConfig>

					<foreign_table>tx_slubevents_domain_model_category</foreign_table>
					<foreign_table_where></foreign_table_where>
					<minitems>1</minitems>
					<maxitems>1</maxitems>
				</config>
			</TCEforms>
		</settings.categorySelection>

		<settings.pidListing>
			<TCEforms>
				<exclude>1</exclude>
				<label>Listing Page</label>
				<config>
					<type>group</type>
					<internal_type>db</internal_type>
					<allowed>pages</allowed>
					<size>1</size>
					<maxitems>1</maxitems>
					<minitems>0</minitems>
					<show_thumbs>1</show_thumbs>
				</config>
			</TCEforms>
		</settings.pidListing>

		<settings.pidDetails>
			<TCEforms>
				<exclude>1</exclude>
				<label>Details Page</label>
				<config>
					<type>group</type>
					<internal_type>db</internal_type>
					<allowed>pages</allowed>
					<size>1</size>
					<maxitems>1</maxitems>
					<minitems>0</minitems>
					<show_thumbs>1</show_thumbs>
				</config>
			</TCEforms>
		</settings.pidDetails>

		<settings.pidSubscribeForm>
			<TCEforms>
				<exclude>1</exclude>
				<label>Subscribe Form</label>
				<config>
					<type>group</type>
					<internal_type>db</internal_type>
					<allowed>pages</allowed>
					<size>1</size>
					<maxitems>1</maxitems>
					<minitems>0</minitems>
					<show_thumbs>1</show_thumbs>
				</config>
			</TCEforms>
		</settings.pidSubscribeForm>
      
     </el>
   </ROOT>
  </sDEF>
 </sheets>
</T3DataStructure>
